@model IEnumerable<ApplicationUser>
@{
    ViewData["Title"] = "Чатик";
    Layout = "~/Views/Shared/ChatLayout.cshtml";
}
<div class="chat-body">
    @foreach (var user in Model)
    {
        <div>
            <a asp-controller="Chat" asp-action="CreatePrivateRoom" asp-route-userId="@user.Id.ToString()" asp-route-namesChat="@user.FirstName.ToString()">
                @user.FirstName
            </a>
        </div>
    }
</div>


